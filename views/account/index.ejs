<% if (title) { %>
  <h1 class="center-title"><%= title %></h1>
<% } else {
  res.redirect('/')
} %>


<%- messages() %>

<% if (errors) { %>
  <ul class="notice">
  <% errors.array().forEach(error => { %>
    <li><%= error.msg %></li>
  <%  }) %>
  </ul>
<% } %>

<div class="account-page-content">
  <p>
    You're logged in
  </p>
</div>
